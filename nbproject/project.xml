            <word>defragmenter</word>
            <word>http</word>
            <word>Impred</word>
            <word>isoperimetric</word>
            <word>morphing</word>
            <word>org</word>
            <word>Paolo</word>
            <word>polyline</word>
            <word>sfdp</word>
            <word>Simonetto</word>
            <word>uncrossable</word>
            <word>www</word>
        </spellchecker-wordlist>
    </configuration>
</project>
